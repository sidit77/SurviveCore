    <PackageReference Include="SharpDX" Version="4.2.0-ci284" />
    <PackageReference Include="SharpDX.Direct3D11" Version="4.2.0-ci284" />
    <PackageReference Include="SharpDX.DXGI" Version="4.2.0-ci284" />
    <PackageReference Include="System.Numerics.Vectors" Version="4.5.0-preview2-26406-04" />
    <PackageReference Include="WinApi" Version="4.0.0" />
    <PackageReference Include="WinApi.Desktop" Version="4.0.0" />
    <PackageReference Include="WinApi.Windows.Controls" Version="4.0.0" />
  </ItemGroup>
</Project>